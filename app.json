      "required": false
    },
    "WS_PATH": {
      "description": "websocket路径",
      "value": "/5ffaedf9-eda6-4ae5-8900-0332c22f7ac8-vmess",
      "required": true
    }
  },
  "website": "https://github.com/ledsafdssg/hrnxdhi",
